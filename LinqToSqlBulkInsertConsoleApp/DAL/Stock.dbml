<?xml version="1.0" encoding="utf-8"?><Database Class="StockDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="Products">
    <Type Name="Product">
      <Column Name="ID" Type="System.Guid" DbType="UniqueIdentifier" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="Name" Type="System.String" DbType="NVarChar(200)" CanBeNull="true" />
      <Column Name="DateOfManufacture" Type="System.DateTime" DbType="DateTime" CanBeNull="true" />
      <Column Name="Price" Type="System.Decimal" DbType="decimal" CanBeNull="true" />
      <Column Name="Count" Type="System.Int32" DbType="int" CanBeNull="true" />
    </Type>
  </Table>
</Database>